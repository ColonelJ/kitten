def unary (-> Int):
  1

def binary (-> Int Int):
  1 2

def ii ((Int -> Int) ->):
  -> f;
  unary f apply sayInt

def iib ((Int Int -> Bool) ->):
  -> f;
  binary f apply sayBool

def iii ((Int Int -> Int) ->):
  -> f;
  binary f apply sayInt

{(+)} iii
{(&)} iii
{(--)} ii
{(/)} iii
{(=)} iib
{(>=)} iib
{(>)} iib
{(++)} ii
{(<=)} iib
{(<)} iib
{(%)} iii
{(*)} iii
{(!=)} iib
{negInt} ii
{(~)} ii
{(|)} iii
{(-)} iii
{(^)} iii
