data Void {}

data Unit: case Unit

data Option {
  case None ()
  case Some (int)
}

data Either {
  case Left (int)
  case Right ([char])
}

data These:
  case This (int)
  case That (char)
  case These (int, char)

Unit_Unit match:
  case Unit_Unit:
    "Unit" say

1 Either_Left LR
"foo" Either_Right LR

define LR (Either ->):
  match:
    case Either_Left: drop "Left" say
    case Either_Right: drop "Right" say

1 Option_Some
1 'x' These_These
